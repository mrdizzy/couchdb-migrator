#!/usr/bin/env node
var migrator = require('./../index');

if (process.argv[2] == 'reset') {
 migrator.resetDb(process.argv[3], function(err, response) {
        if(err) {
            console.log("Reset was not successful: ", err);
        } else {
            console.log("Reset was successful!", response);
        }
    });
}